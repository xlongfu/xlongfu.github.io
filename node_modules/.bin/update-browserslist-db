#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/aoaopipi/Documents/GitHub/xlongfu.github.io/node_modules/update-browserslist-db/node_modules:/Users/aoaopipi/Documents/GitHub/xlongfu.github.io/node_modules:/Users/aoaopipi/Documents/GitHub/node_modules:/Users/aoaopipi/Documents/node_modules:/Users/aoaopipi/node_modules:/Users/node_modules:/node_modules:/Users/aoaopipi/Documents/GitHub/xlongfu.github.io/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/aoaopipi/Documents/GitHub/xlongfu.github.io/node_modules/update-browserslist-db/node_modules:/Users/aoaopipi/Documents/GitHub/xlongfu.github.io/node_modules:/Users/aoaopipi/Documents/GitHub/node_modules:/Users/aoaopipi/Documents/node_modules:/Users/aoaopipi/node_modules:/Users/node_modules:/node_modules:/Users/aoaopipi/Documents/GitHub/xlongfu.github.io/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../update-browserslist-db/cli.js" "$@"
else
  exec node  "$basedir/../update-browserslist-db/cli.js" "$@"
fi
